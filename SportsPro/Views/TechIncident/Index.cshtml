@model Technician
@{
    ViewBag.Title = "Get Technician";
}

<h1>Technician</h1>
<form asp-action="List" method="post">
    <select asp-for="TechnicianID" class="form-select"
            asp-items="@(new SelectList(ViewBag.Technicians, "TechnicianID", "Name"))">
        <option value = "0">Select your Technician</option>
    </select>
    <br />
    <button type="submit" class="btn btn-primary">Select</button>
</form>